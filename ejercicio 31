Algoritmo vector_ordenado
	// se definen las variables 
	Definir n1,n2,a,tem Como Entero
	n1 <- 0; n2 <- 0; a <- 0; tem <- 0
	Dimension vectorA[20]
	// entrada
	// proceso 
	Para i<-1 Hasta 20 Hacer
		Escribir 'ingrese numero        ',i
		Leer vectorA[i]
	FinPara
	Para i<-1 Hasta 19 Hacer
		Para a<-1 Hasta 19 Hacer
			Si vectorA[a]>vectorA[a+1] Entonces
				tem <- vectorA[a]
				vectorA[a] <- vectorA[a+1]
				vectorA[a+1]<-tem
			FinSi
		FinPara
	FinPara
	Para i<-1 Hasta 20 Hacer
		Escribir vectorA[i]
	FinPara
FinAlgoritmo
