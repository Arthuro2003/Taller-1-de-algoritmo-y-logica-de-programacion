Algoritmo buscar_en_vector
	// se definen las variables 
	Definir n1 Como Entero
	n1 <- 0
	// entrada 
	Escribir 'ingrese cantidad a digitar'
	Leer n1
	Dimension vector[n1]
	// proceso 
	Para i<-1 Hasta n1 Hacer
		Escribir 'ingrese digitos'
		Leer vector[i]
	FinPara
	Escribir 'que numero desea buscar?'
	Leer n2
	Para i<-1 Hasta n1 Hacer
		Si n2=vector[i] Entonces
			Escribir 'el numero si se encuentra en el vector'
			c <- 1
		FinSi
	FinPara
	Si c=0 Entonces
		Escribir 'el numero no se encuentra en el vector'
	FinSi
FinAlgoritmo
